{
    "name": "foundry/system",
    "description": "A system plugin for the Foundry Framework.",
    "version": "1.0.0",
    "keywords": [
        "foundry",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "foundry-core",
    "authors": [
        {
            "name": "Co-Foundry",
            "email": "digital@co-foundry.co.za"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/fndry-io/foundry-core.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/yaml": "^4.3",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "rlanvin/php-rrule": "^2.1",
        "spatie/laravel-permission": "^3.2",
        "intervention/image": "^2.5",
        "barryvdh/laravel-cors": "^0.11.4",
        "fideloper/proxy": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Foundry\\System\\Providers\\SystemServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Foundry\\System\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
