{
  "name": "foundry/system",
  "description": "A system plugin for the Foundry Framework.",
  "keywords": [
    "foundry",
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "foundry-core",
  "authors": [
    {
      "name": "Co-Foundry",
      "email": "digital@co-foundry.co.za"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/fndry-io/foundry-core.git"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel-doctrine/acl": "1.0.*",
    "laravel/framework": "5.8.*",
    "symfony/yaml": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Foundry\\System\\Providers\\SystemServiceProvider"
      ],
      "aliases": {
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Foundry\\System\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  }
}
